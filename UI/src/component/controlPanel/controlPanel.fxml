<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>

<ToolBar fx:id="controlPanel" prefHeight="41.0" prefWidth="209.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="component.controlPanel.ControlPanelController">
  <items>
    <Button fx:id="runButton" mnemonicParsing="false" onAction="#handleRun" text="Run" />
      <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#handlePause" text="Pause" />
      <Button fx:id="resumeButton" mnemonicParsing="false" onAction="#handleResume" text="Resume" />
      <Button fx:id="stopButton" mnemonicParsing="false" onAction="#handleStop" text="Stop" />
  </items>
</ToolBar>
